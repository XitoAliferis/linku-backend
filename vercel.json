{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ],
  "crons": [
    {
      "path": "/api/match",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/agent_post",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/update_memory",
      "schedule": "0 4 * * *"
    }
  ]
}
